// question 1

string = "Hello my, name is Raj"
    
    
    
for x in (string):
    if(x==","):
        x="."

print(string)
        

// question 2
string = "Hello my name is Raj"
count=0
myset=set()
for x in (string):
    if(x=="a"or x=="e" or x=="i"or x=="o" or x=="u" or x=="A" or x=="E" or x=="I" or x=="O" or x=="U"):
        myset.add(x)
        count=count+1

print ("Vowels :",count)

print(myset)
    
